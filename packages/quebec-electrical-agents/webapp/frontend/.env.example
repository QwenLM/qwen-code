# Quebec Electrical Agents - Frontend Configuration

# Backend API URL
NEXT_PUBLIC_API_URL=http://localhost:8000

# WebSocket URL (if different from API)
NEXT_PUBLIC_WS_URL=ws://localhost:8000

# Application
NEXT_PUBLIC_APP_NAME="Agents Électriques Québec"
NEXT_PUBLIC_APP_VERSION="1.0.0"

# Features
NEXT_PUBLIC_ENABLE_PGI_DASHBOARD=true
NEXT_PUBLIC_ENABLE_PHOTO_GPS=true
NEXT_PUBLIC_ENABLE_PLAN_ANALYSIS=true

# Map configuration (for photo GPS)
NEXT_PUBLIC_DEFAULT_MAP_CENTER_LAT=45.5017
NEXT_PUBLIC_DEFAULT_MAP_CENTER_LON=-73.5673

# Development
NEXT_PUBLIC_DEV_MODE=true
