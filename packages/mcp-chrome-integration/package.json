{
  "name": "@qwen-code/mcp-chrome-integration",
  "version": "2.0.0",
  "private": true,
  "author": "Qwen Team",
  "description": "Qwen Code Chrome Integration - Native Messaging based MCP bridge",
  "type": "module",
  "scripts": {
    "build:native": "npm --prefix app/native-server run build",
    "build:extension": "npm --prefix app/chrome-extension run build",
    "build": "npm run build:native && npm run build:extension",
    "dev:native": "npm --prefix app/native-server run dev",
    "dev:extension": "npm --prefix app/chrome-extension run dev",
    "dev": "npm run build && npm run dev:native",
    "install:native": "cd app/native-server && node dist/cli.js register",
    "uninstall:native": "cd app/native-server && node dist/cli.js unregister",
    "doctor": "cd app/native-server && node dist/cli.js doctor",
    "lint": "npm --prefix app/native-server run lint",
    "lint:fix": "npm --prefix app/native-server run lint:fix",
    "format": "npm --prefix app/native-server run format",
    "clean:dist": "npm --prefix app/native-server run clean && npm --prefix app/chrome-extension run clean",
    "clean:modules": "rm -rf node_modules app/native-server/node_modules app/chrome-extension/node_modules",
    "clean": "npm run clean:dist && npm run clean:modules",
    "typecheck": "npm --prefix app/native-server run typecheck"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.25.1",
    "@typescript-eslint/eslint-plugin": "^8.32.0",
    "@typescript-eslint/parser": "^8.32.0",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-vue": "^10.0.0",
    "globals": "^16.1.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.1",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.0",
    "vue-eslint-parser": "^10.1.3"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,vue}": [
      "eslint --fix",
      "prettier --write"
    ],
    "**/*.{json,md,yaml,html,css}": [
      "prettier --write"
    ]
  }
}
