{
  "name": "qwen-cli-chrome-bridge-host",
  "version": "0.0.2",
  "description": "Bridge service + MCP server for the Qwen Chrome Extension (HTTP bridge, browser MCP tools)",
  "main": "dist/host.js",
  "bin": {
    "browser-bridge-mcp": "dist/browser-mcp-server.js",
    "chrome-browser-mcp": "dist/browser-mcp-server.js",
    "qwen-bridge-host": "dist/host.js"
  },
  "scripts": {
    "dev": "tsc --project tsconfig.dev.json --watch",
    "build": "npm run clean && tsc --project tsconfig.build.json",
    "clean": "node -e \"require('fs').rmSync('dist', { recursive: true, force: true })\"",
    "start": "node dist/host.js",
    "mcp": "node dist/browser-mcp-server.js",
    "test": "node dist/host.js --test",
    "prepare": "npm run build"
  },
  "keywords": [
    "chrome-extension",
    "native-messaging",
    "qwen",
    "cli",
    "bridge",
    "mcp"
  ],
  "author": "",
  "license": "MIT",
  "files": [
    "dist/",
    "src/",
    "scripts/",
    "README.md"
  ],
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {},
  "devDependencies": {}
}
